{
  "author": "UIUX Lab",
  "email": "uiuxlab@gmail.com",
  "name": "poemkit",
  "projectName": "PoemKit",
  "createdInfo": "UIUX Lab (https://uiux.cc)",
  "projectURL": "https://uiux.cc",
  "version": "0.4.1",
  "description": "React toolkit for building a full website that also is a Micro-Frontend Architecture.",
  "main": "dist/js/poemkit.js",
  "directories": {
    "test": "test"
  },
  "proxy": "http://localhost:3000",
  "scripts": {
    "watch": "webpack --watch --progress",
    "start:open": "webpack-dev-server --open http://localhost:3000",
    "deploy": "cross-env NODE_ENV=production pm2 start ecosystem.config.js",
    "destroy": "pm2 stop ecosystem.config.js & pm2 delete ecosystem.config.js",
    "check": "tsc",
    "dev": "cross-env NODE_ENV=development nodemon --require ignore-styles --exec ts-node -r tsconfig-paths/register ./src/server/server.js",
    "build": "cross-env NODE_ENV=production tsc & webpack --progress --mode production",
    "test": "cross-env NODE_ENV=test jest",
    "prettier": "prettier \"./dist/css/poemkit.css\""
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^@/config/(.*)": "<rootDir>/src/config/$1",
      "^@/components/(.*)": "<rootDir>/src/client/components/$1",
      "^@/router/(.*)": "<rootDir>/src/client/router/$1",
      "^@/helpers/(.*)": "<rootDir>/src/client/helpers/$1",
      "^@/services/(.*)": "<rootDir>/src/client/services/$1",
      "^@/reducers/(.*)": "<rootDir>/src/client/reducers/$1",
      "^@/pages/(.*)": "<rootDir>/src/client/views/_pages/$1",
      "^@/actions/(.*)": "<rootDir>/src/client/actions/$1",
      "^@/server/(.*)": "<rootDir>/src/server/$1",
      "^@/store/(.*)": "<rootDir>/src/store/$1"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.(ts|tsx)?$": "ts-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "poemkit"
  },
  "keywords": [
    "typescript",
    "component",
    "components",
    "frontend",
    "react",
    "react-components",
    "ui",
    "kit",
    "ssr",
    "scss",
    "es6",
    "redux",
    "developer-tools",
    "responsive-design",
    "toolkit",
    "component-library",
    "micro-frontend"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xizon/poemkit/issues"
  },
  "homepage": "https://github.com/xizon/poemkit#readme",
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true
  },
  "eslintConfig": {
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": [
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {},
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.2",
    "@types/react": "^18.0.28",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "file-loader": "^6.2.0",
    "glslify-loader": "^2.0.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "include-file-webpack-plugin": "^1.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsdom": "^21.1.0",
    "json-loader": "^0.5.7",
    "log-timestamp": "^0.3.0",
    "mime-types": "^2.1.35",
    "mini-css-extract-plugin": "^2.7.3",
    "nodemon": "^2.0.21",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-loader": "^3.3.0",
    "random-string": "^0.2.0",
    "raw-loader": "^4.0.2",
    "react-test-renderer": "^17.0.2",
    "safe-buffer": "^5.2.1",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "tar": "^6.1.13",
    "terser-webpack-plugin": "^5.3.7",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-middleware": "^6.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@babel/core": "^7.21.0",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-flow": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@babel/register": "^7.21.0",
    "axios": "^1.3.4",
    "babel-plugin-module-resolver": "^5.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "ignore-styles": "^5.0.1",
    "moment": "^2.29.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.7",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.3.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
